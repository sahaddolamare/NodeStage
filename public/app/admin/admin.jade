.container
  .well
    h3 Stage Administration
      span.iconic.iconic-cogs.iconic-lg.iconic-size-md.h3-icon
    .row
      .col-md-9
        h4
          b Loaded Stage users
        .table-responsive
          table.table.table-bordered.table-condensed.table-striped
            tr
              th Username
              th First name
              th Last name
              th Roles
            tr(ng-repeat="user in users")
              td {{user.userName}}
              td {{user.firstName}}
              td {{user.lastName}}
              td {{user.roles}}
      .col-md-3
        h4
          b User actions
        div.list-group
          button.list-group-item.disabled Create new user
            span.iconic.iconic-plus-thin.iconic-sm.iconic-size-sm.lgr-icon
          button.list-group-item.disabled Purge user database
            span.iconic.iconic-radiation.iconic-sm.iconic-size-sm.lgr-icon
        